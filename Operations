#include <stdio.h>
void main(){
int a;
int b;
printf("Enter the first number :");
scanf("%d",&a);
printf("Enter the second number :");
scanf("%d",&b);
printf("Addition:%d",a+b);
printf("\nSubtraction:%d",a-b);
printf("\nMultiplication:%d",a*b);
if(b!=0)
  {
    printf("\nDivision:%d",a/b );
  }
else
  {
    printf("\nInvalid number");
  }
